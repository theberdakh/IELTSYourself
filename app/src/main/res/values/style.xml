<?xml version="1.0" encoding="utf-8"?>
<resources>


    <style name="ShapeAppearanceOverlay.App" parent="">
        <item name="cornerSize">10%</item>
    </style>
    <style name="ShapeAppearanceOverlayProfile" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="SettingsTextView" parent="Widget.AppCompat.TextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">@dimen/dimen_8</item>
        <item name="android:textColor">?colorOnSurface</item>
        <item name="android:textSize">@dimen/text_14</item>
        <item name="android:drawablePadding">@dimen/dimen_8</item>
    </style>

    <style name="WordChip" parent="ShapeAppearanceOverlay.Material3.Chip">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">@dimen/dimen_8</item>
        <item name="android:padding">@dimen/dimen_8</item>
    </style>

</resources>